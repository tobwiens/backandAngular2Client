{
  "name": "backand-angular2-client",
  "version": "0.0.2",
  "description": "Backand Angular2 Client",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server:dev": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached  --content-base src/",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server",
    "test": "tsc && karma start karma.conf.js",
    "report": "lite-server -c bs-config.report.json",
    "clean": "rimraf coverage && rimraf report && rimraf dist && rimraf test-output",
    "cleanNodeModules": "rimraf node_modules",
    "posttest": "node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage",
    "lint": "tsconfig-lint .",
    "coverage-report": "rimraf coverage && rimraf dist && rimraf test-output && npm run test && npm run posttest && npm run coverage"
  },
  "contributors": [
  ],
  "license": "MIT",
  "devDependencies": {
    "browserify": "13.1.0",
    "karma-browserify": "5.1.0",
    "ncp": "2.0.0",
    "node-sass": "3.7.0",
    "rimraf": "2.5.1",
    "ts-helper": "0.0.1",
    "ts-node": "0.5.5",
    "tsconfig-lint": "0.12.0",
    "typedoc": "0.3.12",
    "typings": "1.0.4",
    "jasmine-core": "~2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "protractor": "^3.3.0",
    "systemjs": "0.19.27",
    "remap-istanbul": "0.7.0",
    "karma-coverage": "1.1.1",
    "browserify-istanbul": "2.0.0",
    "istanbul": "0.4.5",
    "reflect-metadata": "0.1.2",
    "http-server": "0.8.5"
  },
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/router": "3.1.1",
    "typescript": "2.0.3",
    "@angular/platform-browser-dynamic": "2.0.0",
    "autoprefixer": "6.5.1",
    "angular2-template-loader": "^0.5.0",
    "rxjs": "5.0.0-beta.12",
    "core-js": "^2.4.1"
  },
  "keywords": [
    "Backand",
    "Backand Angular 2",
    "Backand Angular2",
    "Backand Login",
    "Backand SQL",
    "Backand NoSQL",
    "Backand Database"
  ]
}
